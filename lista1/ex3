#include <stdio.h>

int main() {
    int num1, num2, num3, soma;

    printf("Digite o primeiro numero inteiro: ");
    scanf("%d", &num1);

    printf("Digite o segundo numero inteiro: ");
    scanf("%d", &num2);

    printf("Digite o terceiro numero inteiro: ");
    scanf("%d", &num3);

    soma = num1 + num2 + num3;

    printf("A soma dos tres numeros e: %d\n", soma);

    return 0;
}
